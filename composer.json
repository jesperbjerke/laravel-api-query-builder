{
    "name": "bjerke/api-query-builder",
    "description": "A query builder for Laravel that parses the request and uses Eloquent ORM to query database",
    "keywords": [
        "jesperbjerke",
        "querybuilder",
        "api",
        "eloquent",
        "query",
        "builder",
        "orm",
        "rest",
        "laravel"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jesper Bjerke",
            "email": "jesper.bjerke@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 | ^10.0",
        "illuminate/http": "^9.0 | ^10.0",
        "illuminate/database": "^9.0 | ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bjerke\\ApiQueryBuilder\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bjerke\\ApiQueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
